{
  "name": "myweb-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo 'Server build completed'",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:cov": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "migrate": "knex migrate:latest --knexfile ./knexfile.cjs",
    "migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.cjs",
    "seed": "knex seed:run --knexfile ./knexfile.cjs",
    "db:check": "node src/utils/db-check.js",
    "db:setup": "npm run migrate && npm run db:check"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "iconv-lite": "^0.6.3",
    "joi": "^17.11.0",
    "jschardet": "^3.0.0",
    "knex": "^2.5.1",
    "multer": "^2.0.2",
    "music-metadata": "^7.14.0",
    "sharp": "^0.33.4",
    "tar-fs": "^2.1.4",
    "uuid": "^9.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.4"
  }
}
