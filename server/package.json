{
  "name": "myweb-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:cov": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "migrate": "knex migrate:latest --knexfile ./knexfile.cjs",
    "migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.cjs",
    "seed": "knex seed:run --knexfile ./knexfile.cjs"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.0",
    "better-sqlite3": "^9.2.0",
    "multer": "^1.4.4",
    "joi": "^17.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "jest": "^29.7.0",
    "knex": "^2.5.1"
  }
}
